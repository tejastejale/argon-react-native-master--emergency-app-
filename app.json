{
  "expo": {
    "owner": "tejas13",
    "name": "Argon FREE React Native",
    "slug": "argon-free-react-native",
    "privacy": "public",
    "platforms": ["ios", "android"],
    "version": "1.7.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.argonfreereactnative"
    },

    "plugins": [
      [
        "@rnmapbox/maps",
        {
          "RNMapboxMapsVersion": "10.1.33",
          "RNMapboxMapsDownloadToken": "sk.eyJ1IjoidGVqYXNjb2RlNDciLCJhIjoiY200eDAxYWg0MGRkYjJpczh3bGxjNWNlZiJ9.NpkF0o8U_yLwo8oXeG7xFw"
        }
      ]
    ],
    "description": "Argon React Native, based on Argon Design System. Coded by Creative Tim",
    "android": {
      "usesCleartextTraffic": true,
      "package": "com.anonymous.argonfreereactnative",
      "permissions": ["ACCESS_FINE_LOCATION"]
    },
    "extra": {
      "eas": {
        "projectId": "fe193360-417d-440e-a1f9-5d090db826c0"
      }
    }
  }
}
